    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "cssnano": "^3.10.0",
    "debug": "~2.6.3",
    "del": "^3.0.0",
    "express": "~4.15.2",
    "gulp": "^3.9.1",
    "gulp-flatten": "^0.4.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-plumber": "^1.2.0",
