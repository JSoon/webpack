{
  "name": "scaffold",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "wp:dev": "./node_modules/webpack/bin/webpack.js --config build/webpack.dev.js --progress --color --display-error-details",
    "wp:dev:win": "node ./node_modules/webpack/bin/webpack.js --config build/webpack.dev.js --progress --color --display-error-details",
    "wp:prod": "./node_modules/webpack/bin/webpack.js --config build/webpack.prod.js --progress --color --display-error-details",
    "wp:prod:win": "node ./node_modules/webpack/bin/webpack.js --config build/webpack.prod.js --progress --color --display-error-details",
    "gp:dev": "gulp --gulpfile build/gulpfile.js development",
    "gp:prod": "gulp --gulpfile build/gulpfile.js production"
  },
  "engines": {
    "node": "8.11.1",
    "npm": "5.6.0"
  },
  "browserslist": [
    "> 5% in CN",
    "Last 2 versions",
    "ie >= 8"
  ],
