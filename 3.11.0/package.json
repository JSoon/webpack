    "gulp-watch": "^5.0.0",
    "morgan": "~1.8.1",
    "postcss-url": "^7.3.2",
    "precss": "^3.1.2",
    "pug": "~2.0.0-beta11",
    "pug-loader": "^2.4.0",
    "serve-favicon": "~2.4.2",
